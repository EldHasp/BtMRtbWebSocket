<Application x:Class="BTMRTB.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BTMRTB"
             xmlns:converters="clr-namespace:CommLibrary.Converters;assembly=CommLibrary"
             Startup="Application_Startup">
              <!--StartupUri="KeySecret.xaml"-->
   <Application.Resources>
        <Style TargetType="FrameworkElement" x:Key="ComStyle">
            <Setter Property="Margin" Value="5"/>
        </Style>
        <converters:BooleanNotConverter x:Key="BooleanNotConverter"/>
        <converters:NullableBoolToStringConverter x:Key="NullableBoolToStringConverter"/>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    </Application.Resources>
</Application>
